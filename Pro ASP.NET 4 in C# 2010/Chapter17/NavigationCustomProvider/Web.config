<?xml version="1.0"?>

<configuration>
	<appSettings/>
	<connectionStrings/>
	<system.web>
		<siteMap defaultProvider="SqlSiteMapProvider">
			<providers>
				<add name="SqlSiteMapProvider" type="SqlSiteMapProvider" providerName="System.Data.SqlClient" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=Northwind;Integrated Security=SSPI" storedProcedure="GetSiteMap" cacheTime="600"/>
			</providers>
		</siteMap>
		
    
		<compilation debug="true" targetFramework="4.0">
		</compilation>
  </system.web>
	
</configuration>
