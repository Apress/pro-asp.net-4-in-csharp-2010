<?xml version="1.0"?>

<configuration>
	<appSettings/>
	<connectionStrings>
		<add name="Northwind" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=Northwind;Integrated Security=SSPI"/>
	</connectionStrings>
  
	<system.web>		
		<compilation debug="true" targetFramework="4.0">
			<expressionBuilders>
				<add expressionPrefix="RandomNumber" type="RandomNumberExpressionBuilder"/>
			</expressionBuilders>			
		</compilation>
		
	</system.web>
	
</configuration>
